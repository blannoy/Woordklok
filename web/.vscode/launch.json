    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Dev start",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "start"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Acc start",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "start:acc"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Dev build",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "build"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Prod build",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "build:production"
            ]
        }
    ]

}